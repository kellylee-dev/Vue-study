{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "plugin:vue/essential",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "eqeqeq": "off",
        "curly": "warn",
     //   "quotes": ["warn", "double"],
        "vue/no-multiple-template-root": "off" 
    }
}
